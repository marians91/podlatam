{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Base Solution JSON Schema",
    "properties": {
        "solutionId": {
            "type": "string"
        },
        "solution": {
            "type": "string"
        },
        "userAdmin": {
            "type": "object",
            "properties": {
                "userId": {
                    "type": "string"
                },
                "surname": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                }
            },
            "required": [
                "userId",
                "surname",
                "name"
            ]
        }
    },
    "required": [
        "solutionId",
        "solution",
        "userAdmin"
    ]
}